  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "processmd": "^4.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
