{
  "name": "jointanet",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "nob <yanbe.n@jointanet.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
